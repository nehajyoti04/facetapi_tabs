{#
/**
 * @file
 * Default theme implementation of a facetapi result item.
 */
#}

{% if is_active %}
  <span class="facet-item__status js-facet-deactivate">(-)</span>
{% endif %}
<span class="facet-item__value">{{ value }}</span>
{% if show_count %}
  <span class="facet-item__count">({{ count }})</span>
{% endif %}
